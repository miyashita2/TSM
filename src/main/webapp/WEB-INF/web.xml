<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>jp.main.servlet.SearchServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>jp.main.servlet.UpdateServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>insertServlet</servlet-name>
    <servlet-class>jp.main.servlet.insertServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/SearchServlet</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>UpdateServlet</servlet-name>
    <url-pattern>/UpdateServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>insertServlet</servlet-name>
    <url-pattern>/insertServlet</url-pattern>
  </servlet-mapping>
</web-app>
